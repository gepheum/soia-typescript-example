  "scripts": {
    "build": "soiac && tsc",
    "bundle": "esbuild src/client.ts --bundle --outfile=static/bundle.js --platform=browser --format=iife",
    "server": "npm run build && npm run bundle && node dist/src/server.js",
    "client-on-node": "npm run build && node dist/src/client.js",
    "snippets": "npm run build && node dist/src/snippets.js",
    "test": "mocha",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@types/express": "^5.0.1",
    "express": "^5.1.0",
    "soia": "^1.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "buckwheat": "^1.1.2",
    "esbuild": "^0.24.2",
    "mocha": "^11.1.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^4.2.0",
    "soia-typescript-gen": "^1.2.1",
    "soiac": "^1.0.51",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
