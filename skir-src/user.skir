/// A user
struct User {
  user_id: int32;
  name: string;
  /// A personal quote or motto chosen by the user
  quote: string;

  struct Pet {
    name: string;
    height_in_meters: float32;
    picture: string;
  }
  pets: [Pet];

  subscription_status: SubscriptionStatus;
}

enum SubscriptionStatus {
  FREE;
  trial: Trial;
  PREMIUM;

  struct Trial {
    start_time: timestamp;
  }
}

